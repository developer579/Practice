MyCanvas:

<MyCanvas@Widget>:
	canvas:
		Line:
			points:0,50,400,50,400,250,100,250,100,0
			width:1
			dash_length:5
			dash_offset:2.5
		Ellipse:
			pos:100,50
			size:300,200
	Label:
		text:"(100,50)"
		pos:0,-15
		font_size:25
		bold:True
	Label:
		text:"300"
		pos:200,-15
		font_size:25
	Label:
		text:"200"
		pos:25,95
		font_size:25